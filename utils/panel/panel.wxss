
.panel{
  background: #0c1b2ead;
  width: 94%;
  height: 94%;
  position: fixed;
  margin-left: 3%;
  margin-top: 3%;
  box-sizing: border-box;
  border: 1px solid #2C58A6;
  box-shadow: 0 0 10px #2C58A6;
   z-index: -1;
}
.l_t_line{
    width: 5px;
    height: 34px;
    left: -3px;
    top: -3px;
}
.t_l_line{
    height: 5px;
    width: 26px;
    left: -3px;
    top: -3px;
}
.t_line_box {
    position: absolute;
    width: 100%;
    height: 100%;
}
.t_line_box icon{
    background-color: #4788fb;
    box-shadow: 0px 0px 10px #4788fb;
    position: absolute;
}
.t_r_line{
    height: 5px;
    width: 26px;
    right: -3px;
    top: -3px;
}
.r_t_line{
    width: 5px;
    height: 34px;
    right: -3px;
    top: -3px;
}
.l_b_line{
    width: 5px;
    height: 34px;
    left: -3px;
    bottom: -3px;
}
.b_l_line{
    height: 5px;
    width: 26px;
    left: -3px;
    bottom: -3px;
}
.r_b_line{
    width: 5px;
    height: 34px;
    right: -3px;
    bottom: -3px;
}
.b_r_line{
    height: 5px;
    width: 26px;
    right: -3px;
    bottom: -3px;
}
